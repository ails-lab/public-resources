{
 "@context": {
    "as": "https://www.w3.org/ns/activitystreams#",
    "prov": "http://www.w3.org/ns/prov#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "soa": "http://sw.islab.ntua.gr/annotation/",

    "GraphBody": "soa:GraphBody",
    "Rating": "soa:Rating",
    "Validation": "soa:Validation",
    "confidence": {"@id": "soa:confidence", "@type": "xsd:decimal"},
    "recommendation": {"@type": "@vocab", "@id": "soa:recommendation"},
    "review": {"@type": "@id", "@id": "soa:hasReview"},
    "statements": {"@id": "soa:statements", "@container": "@graph"},
    "asserting": "soa:asserting",
    "accept": "soa:Accept",
    "reject": "soa:Reject",
	 
    "Literal": "soa:Literal",
    "RDFPathSelector": "soa:RDFPathSelector",
    "RDFPropertySelector": "soa:RDFPropertySelector",
    "destination": {"@type": "@id", "@id": "soa:destination"},
    "property": {"@id": "soa:property", "@type": "@id" },
    "rdfPath": "soa:rdfPath",
    "score": {"@id": "soa:score", "@type": "xsd:decimal"},
	
    "wasDerivedFrom": "prov:wasDerivedFrom",
	
    "generator": {"@type": "@id", "@id": "as:generator"},
    "Software": "as:Application"
  }
}
